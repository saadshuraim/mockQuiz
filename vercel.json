{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api": {
      "runtime": "@vercel/node@latest",
      "entrypoint": "server/index.ts",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}